{
  "manifestVersion": 1,
  "id": "ado-dashboard-widgets",
  "version": "1.0.0",
  "name": "Dashboard Widgets",
  "description": "Various ADO Dashboard Widgets",
  "publisher": "stefanjarina",
  "categories": ["Azure Boards"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "img/logo.png"
  },
  "contributions": [
    {
      "id": "ActivePrsWithClosedWitsWidget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": ["ms.vss-dashboards-web.widget-catalog"],
      "properties": {
        "name": "Active PRs Widget",
        "description": "List PRs with closed WITs",
        "catalogIconUrl": "img/logo.png",
        "previewImageUrl": "img/logo.png",
        "uri": "active-prs-with-closed-wits.html",
        "supportedSizes": [
          {
            "rowSpan": 1,
            "columnSpan": 2
          }
        ],
        "supportedScopes": ["project_team"]
      }
    }
  ],
  "files": [
    {
      "path": "active-prs-with-closed-wits.html",
      "addressable": true
    },
    {
      "path": "sdk/scripts",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    }
  ],
  "scopes": ["vso.code"]
}
